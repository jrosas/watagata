define sum(a: num, b: num) of type num as
    return a+b

begin
    vars
        one: num;
        two: num;
        three: num;
        caramba: vec

    one := 1;
    two := 2;
    three := sum(one,two);
    caramba := range(2**2);

    while $caramba < 10 do
    begin
        vars
            wepa: mat
        wepa := {1,2,3;4,5,6;7,8,9};
        one := sum(one,two)*caramba[2];
        two := caramba.caramba;
        caramba := wepa[:1,:] * caramba
    end;

    write wepa[one+two]
end
